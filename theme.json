{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "palette": [
        {
          "slug": "dark-honey-brown",
          "color": "#5A2A00",
          "name": "Dark Honey Brown"
        },
        {
          "slug": "warm-amber",
          "color": "#FFB84D",
          "name": "Warm Amber"
        },
        {
          "slug": "soft-gold",
          "color": "#FFD27A",
          "name": "Soft Gold"
        },
        {
          "slug": "button-orange",
          "color": "#FF8C00",
          "name": "Button Orange"
        },
        {
          "slug": "card-border",
          "color": "#FFE4B5",
          "name": "Card Border"
        },
        {
          "slug": "white",
          "color": "#FFFFFF",
          "name": "White"
        }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Arial, sans-serif",
          "slug": "arial",
          "name": "Arial"
        },
        {
          "fontFamily": "'Times New Roman', serif",
          "slug": "times-new-roman",
          "name": "Times New Roman"
        }
      ]
    },
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1400px"
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--soft-gold)",
      "text": "var(--wp--preset--color--dark-honey-brown)"
    },
    "blocks": {
      "core/button": {
        "color": {
          "background": "var(--wp--preset--color--button-orange)",
          "text": "var(--wp--preset--color--white)"
        }
      },
      "core/group": {
        "color": {
          "background": "var(--wp--preset--color--soft-gold)"
        }
      }
    }
  }
}